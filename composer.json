{
    "name": "goldfinch/harvest",
    "description": "Harvest is a seeder component that helps you to bundle automation run by Taz",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "harvest",
        "harvest-silverstripe",
        "seeder",
        "seeder-silverstripe",
        "harvest-manager"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Art Karasev",
            "email": "art.karasev@proton.me"
        }
    ],
    "require": {
        "php": ">=8.0",
        "silverstripe/framework": "^5.0",
        "silverstripe/admin": "^2.0",
        "goldfinch/taz": "^2.0",
        "goldfinch/cli-supplier": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Goldfinch\\Harvest\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
